{#
/**
 * @file
 * Template for topic list builder paragraphs topic list links field.
#}
<div{{ attributes }}>
  {% for row in items|batch(2) %}
  <div class="row">
    {% for item in row %}
    <div class="col-sm-6">
      {{ item.content }}
    </div>
    {% endfor %}
  </div>
  {% endfor %}
</div>
